{
  "name": "eth-tech-tree-backend",
  "version": "0.0.1",
  "description": "backend for managing users and challenges for the ETH Tech Tree",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/server"
    ]
  },
  "scripts": {
    "build": "yarn workspace @eth-tech-tree-backend/server build",
    "start": "yarn workspace @eth-tech-tree-backend/server start",
    "dev": "yarn workspace @eth-tech-tree-backend/server dev",
    "install:challenges": "yarn workspace @eth-tech-tree-backend/server install:challenges"
  },
  "packageManager": "yarn@3.2.3",
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  }
}
